<!DOCTYPE html>
<html>
<head>
  <title>Twin Falls Gazette</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if notice %>
	<p class="notice"><%= notice %></p>
	<% end %>
	<% if alert %>
    <p class="alert"><%= alert %></p>
    <% end %>
	<div id="masthead">
		<div id="paper_name">
			<p><h1>Twin Falls Gazette</h1></p>
			<p><h3>Serving Twin Falls and the Grey Mountains</h3></p>
		</div>
		<div id="login_area">
		</div>
	</div>
	<div id="article_section">
		<%= yield %>
	</div>
</body>
</html>
